{
  "name": "vue3-sorted-table",
  "description": "A Vue3 plugin to sort table content.",
  "author": "Benjamin Flesch",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue.js",
    "vue3",
    "nuxt",
    "sort",
    "plugin",
    "table",
    "sorted",
    "component",
    "vue-sorted-table"
  ],
  "version": "1.0.3",
  "main": "./src/index.js",
  "jsnext:main": "./src/index.js",
  "module": "./src/index.js",
  "repository": "git@github.com:bf/vue3-sorted-table.git",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix"
  },
  "files": [
    "src/*",
    "*.json",
    "*.js"
  ],
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.43.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "prettier": "^2.8.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended",
      "@vue/prettier"
    ],
    "rules": {
      "vue/no-v-html": "off",
      "no-unused-vars": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
